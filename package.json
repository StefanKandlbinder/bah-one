{
  "name": "one-bah",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"style-dictionary build\" \"ng serve -o\" \"npm run storybook\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build-button": "ng build colibri-button --prod",
    "pack-button": "cd dist/button && npm pack",
    "pack-core": "cd colibri/core && npm pack",
    "package": "npm run build-button && npm run pack-button",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "tailwind-angular": "ng config projects.one-bah.architect.build.builder @angular-builders/custom-webpack:browser && ng config projects.one-bah.architect.build.options.customWebpackConfig.path webpack.config.js && ng config projects.one-bah.architect.serve.builder @angular-builders/custom-webpack:dev-server && ng config projects.one-bah.architect.test.builder @angular-builders/custom-webpack:browser && ng config projects.one-bah.architect.test.options.customWebpackConfig.path webpack.config.js"
  },
